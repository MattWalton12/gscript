<% include ../partials/header %>
<div class="container notifications">
	<% for (i=0; i<notifications.length; i++) { %>
		<div class="row">
			<div class="col-md-8">
				<div class="notification <%=(notifications[i].read && "read" || "")%>">
					<span class="date">(<%=moment(notifications[i].date).fromNow()%>)</span>
					<span class="message"><%-notifications[i].text%></span>
				</div>
			</div>
		</div>
	<% } %>
</div>
<% include ../partials/footer %>
