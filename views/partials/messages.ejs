<div class="container messages">
<% if (messages.error) { %>
	<% for (i=0; i<messages.error.length; i++) { %>
		<div class="alert alert-danger"><%=messages.error[i]%></div>
	<% } %>
<% } %>

<% if (messages.info) { %>
	<% for (i=0; i<messages.info.length; i++) { %>
		<div class="alert alert-info"><%=messages.info[i]%></div>
	<% } %>
<% } %>
</div>